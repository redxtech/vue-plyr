{
  "name": "@skjnldsv/vue-plyr",
  "version": "7.0.1-alpha.1",
  "description": "A vue component for the plyr video & audio player.",
  "license": "MIT",
  "author": {
    "name": "Gabe Dunn",
    "email": "gabe@redxtech.ca",
    "url": "https://gabedunn.dev"
  },
  "repository": "github:skjnldsv/vue-plyr",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "dev": "echo 'No dev build available, production only' && npm run build",
    "watch": "rollup --config rollup.config.js --watch",
    "lint": "eslint --ext .js,.vue",
    "lint:fix": "eslint --ext .js,.vue --fix"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "plyr": "^3.6.8",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@nextcloud/babel-config": "^1.0.0",
    "@nextcloud/eslint-config": "^8.0.0",
    "@nextcloud/stylelint-config": "^2.1.2",
    "@rollup-extras/plugin-clean": "^1.2.3",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@vue/tsconfig": "^0.1.3",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.1",
    "rollup": "^2.75.6",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-plugin-vue2": "^0.8.1",
    "sass": "^1.52.3"
  },
  "keywords": [
    "vue-plyr",
    "vue",
    "plyr",
    "video",
    "audio",
    "youtube",
    "vimeo",
    "media",
    "player",
    "js",
    "javascript",
    "rollup"
  ]
