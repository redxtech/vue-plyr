{
  "name": "vue-plyr",
  "version": "5.1.2",
  "description": "A set of Vue components for the plyr video & audio player.",
  "main": "dist/vue-plyr.js",
  "module": "dist/vue-plyr.mjs",
  "jsnext:main": "dist/vue-plyr.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/demo/vue-cli/main.js",
    "demo:vue-cli": "vue-cli-service serve src/demo/vue-cli/main.js",
    "demo:nuxt": "nuxt -c src/demo/nuxt/nuxt.config.js",
    "demo:nuxt:spa": "nuxt -c src/demo/nuxt/nuxt.config.js -s",
    "cli-build": "vue-cli-service build --target lib src/index.js",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "prebuild": "rimraf dist",
    "build": "bili"
  },
  "author": {
    "name": "Gabe Dunn",
    "email": "gabe@redxtech.ca",
    "url": "https://redxte.ch"
  },
  "license": "MIT",
  "repository": "github:redxtech/vue-plyr",
  "keywords": [
    "vue",
    "plyr",
    "video",
    "audio",
    "youtube",
    "vimeo",
    "media",
    "player",
    "js",
    "javascript",
    "bili"
  ],
  "dependencies": {
    "plyr": "^3.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-jest": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "bili": "^4.5.2",
    "cross-env": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "nodemon": "^1.18.9",
    "nuxt": "^2.3.4",
    "rimraf": "^2.6.3",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-vue": "^4.6.1",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.22"
  }
}
